    "react": "19.1.0",
    "react-native-builder-bob": "^0.40.13",
    "release-config": "workspace:*",
    "release-it": "^19.0.5",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react-native": "*"
